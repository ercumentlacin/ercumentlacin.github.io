  "private": true,
  "scripts": {
    "dev": "concurrently \"contentlayer dev\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"app/**/*.tsx\"",
